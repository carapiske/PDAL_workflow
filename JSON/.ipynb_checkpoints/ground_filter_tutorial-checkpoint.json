{
    "pipeline": [
        {
            "type": "filters.reprojection",
            "in_srs": "EPSG:4326",
            "out_srs": "EPSG:4326"
        },
        {
            "type": "filters.assign",
            "assignment": "Classification[:]=0"
        },
        {
            "type": "filters.elm"
        },
        {
            "type": "filters.outlier"
        },
        {
            "type": "filters.smrf",
            "ignore": "Classification[7:7]",
            "slope": 0.3,
            "window": 16,
            "scalar": 1
        },
        {
            "type": "filters.range",
            "limits": "Classification[2:2]"
        }
    ]
}